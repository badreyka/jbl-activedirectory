    "badreyka <andrey@ufgca.com>"
  ],
  "main": "index.js",
  "directories": {
    "lib": "lib",
    "test": "test"
  },
  "engines": {
    "node": ">=4.0"
  },
  "keywords": [
    "ldap",
    "active directory"
  ],
  "bugs": {
    "url": "https://github.com/badreyka/jbl-activedirectory/issues"
  },
  "dependencies": {
    "abstract-logging": "^2.0.0",
    "async": "^3.1.0",
    "ldapjs": "^2.3.3",
    "merge-options": "^2.0.0"
  },
  "scripts": {
    "lint": "standard | snazzy",
