    "url": "git+https://github.com/badreyka/jbl-activedirectory.git"
  },
  "license": "MIT",
  "devDependencies": {
    "jsdoc": "^3.6.5",
    "pino": "^6.4.1",
    "pre-commit": "^1.2.2",
